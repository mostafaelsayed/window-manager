{
    "name": "Window Manager",
    "description": "Manager your windows",
    "permissions": [
        "tabs",
        "storage",
        "sidePanel",
        "contextMenus"
    ],
    "manifest_version": 3,
    "version": "1.0",
    "side_panel": {
        "default_path": "side-panel.html"
    },
    "action": {
        "default_title": "Click to open panel"
    }
}